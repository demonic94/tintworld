namespace tinyworld.tinydb;

context tinyf {

    /*@@layout{"layoutInfo":{"x":267,"y":146.5}}*/
    entity world {
        key continent : String(100);
    };

    /*@@layout{"layoutInfo":{"x":-43,"y":73.5}}*/
    entity country {
        name   : String(100);
        partof : association[0..1] to world { continent };
    };
};